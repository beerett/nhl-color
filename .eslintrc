{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "node": true
  },
  "rules": {
    "arrow-parens": "off",
    "consistent-return": "off",
    "camelcase": "off",
    "import/no-dynamic-require": "off",
    "global-require": "off",
    "no-underscore-dangle": "off",
    "no-console": "off",
    "comma-dangle": 0
  },
  "plugins": [
    "babel"
  ]
}
